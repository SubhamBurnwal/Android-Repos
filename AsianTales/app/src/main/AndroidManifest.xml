<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="subham.unprodev.asiantales">

    <application
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.Splash">
        <activity
            android:name="subham.unprodev.asiantales.SplashActivity"
            android:theme="@style/AppTheme.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="subham.unprodev.asiantales.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
        <activity
            android:name="subham.unprodev.asiantales.GameActivity"
            android:label="GameActivity"
            android:parentActivityName="subham.unprodev.asiantales.MainActivity">

            <!--
                <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="subham.unprodev.asiantales.MainActivity" /
            -->
        </activity>
    </application>

</manifest>